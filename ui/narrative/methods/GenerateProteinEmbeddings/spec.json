{
    "ver": "0.0.1",
    "authors": [
        "yourname"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": [
        "active",
        "protein",
        "embedding"
    ],
    "widgets": {
        "input": null,
        "output": null
    },
    "parameters": [
        {
            "id": "protein_id",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [],
                "validate_as": "regex",
                "regex_constraint": "^[A-NR-Z][0-9][A-Z0-9]{3}[0-9]$|^[OPQ][0-9][A-Z0-9]{3}[0-9]$|^[A-Z0-9]{6,10}$",
                "regex_error_message": "Enter a valid UniProt accession (e.g., P12345). Leave blank if providing a sequence."
            }
        },
        {
            "id": "sequence",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "textarea",
            "text_options": {
                "valid_ws_types": [],
                "example": ">ExampleProtein\nMKTAYIAKQRQISFVKSHFSRQDILDLWIYHTQGYFPQ"
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kbase_protein_query_module",
            "method": "generate_protein_embedding",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "sequence",
                    "target_property": "sequence"
                },
                {
                    "input_parameter": "protein_id",
                    "target_property": "protein_id"
                }
            ],
            "output_mapping": [
                { "service_method_output_path": [0, "report_name"], "target_property": "report_name" },
                { "service_method_output_path": [0, "report_ref"], "target_property": "report_ref" },
                { "service_method_output_path": [0, "embedding_result"], "target_property": "embedding_result" },
                { "service_method_output_path": [0, "embedding_result_ref"], "target_property": "embedding_result_ref" }
            ]
        }
    },
    "job_id_output_field": "docker"
}