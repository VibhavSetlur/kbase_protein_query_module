{
    "ver": "0.0.1",
    "authors": [
        "yourname"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": [
        "active",
        "protein",
        "embedding"
    ],
    "widgets": {
        "input": null,
        "output": null
    },
    "parameters": [
        {
            "id": "input_source",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["sequence"],
            "field_type": "dropdown",
            "text_options": {
                "valid_ws_types": [],
                "example": "Choose input source",
                "options": [
                    {"value": "sequence", "display": "Protein Sequence (Direct Input)"},
                    {"value": "workspace_object", "display": "From Protein Check Result"}
                ]
            }
        },
        {
            "id": "protein_sequence",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "textarea",
            "text_options": {
                "valid_ws_types": [],
                "example": ">ExampleProtein\nMKTAYIAKQRQISFVKSHFSRQDILDLWIYHTQGYFPQ\n\nOr just the sequence:\nMKTAYIAKQRQISFVKSHFSRQDILDLWIYHTQGYFPQ"
            }
        },
        {
            "id": "protein_check_result_ref",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "workspaceObjectRef",
            "text_options": {
                "valid_ws_types": ["ProteinExistenceResult"],
                "example": "Select a protein check result containing protein data and metadata"
            }
        },
        {
            "id": "output_name",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["protein_embedding"],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [],
                "example": "Name for the output embedding object (e.g., collagen_embedding, insulin_embedding)"
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kbase_protein_query_module",
            "method": "generate_protein_embedding",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "input_source",
                    "target_property": "input_source"
                },
                {
                    "input_parameter": "protein_sequence",
                    "target_property": "protein_sequence"
                },
                {
                    "input_parameter": "protein_check_result_ref",
                    "target_property": "protein_check_result_ref"
                },
                {
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                }
            ],
            "output_mapping": [
                { "service_method_output_path": [0, "embedding_result_ref"], "target_property": "embedding_result_ref" },
                { "service_method_output_path": [0, "summary"], "target_property": "summary" },
                { "service_method_output_path": [0, "input_parameters"], "target_property": "input_parameters" },
                { "service_method_output_path": [0, "start_time"], "target_property": "start_time" },
                { "service_method_output_path": [0, "embedding_norm"], "target_property": "embedding_norm" },
                { "service_method_output_path": [0, "sequence_length"], "target_property": "sequence_length" },
                { "service_method_output_path": [0, "embedding_dim"], "target_property": "embedding_dim" },
                { "service_method_output_path": [0, "protein_id"], "target_property": "protein_id" },
                { "service_method_output_path": [0, "family_id"], "target_property": "family_id" }
            ]
        }
    },
    "job_id_output_field": "docker"
}