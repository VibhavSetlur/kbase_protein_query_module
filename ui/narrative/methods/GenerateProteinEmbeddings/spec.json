{
    "ver": "0.0.1",
    "authors": [
        "yourname"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": [
        "active",
        "protein",
        "embedding"
    ],
    "widgets": {
        "input": null,
        "output": null
    },
    "parameters": [
        {
            "id": "input_type",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["sequence"],
            "field_type": "dropdown",
            "text_options": {
                "valid_ws_types": [],
                "example": "Choose input type",
                "options": [
                    {"value": "sequence", "display": "Protein Sequence (Text)"},
                    {"value": "workspace_object", "display": "Workspace Object"}
                ]
            }
        },
        {
            "id": "sequence",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "textarea",
            "text_options": {
                "valid_ws_types": [],
                "example": ">ExampleProtein\nMKTAYIAKQRQISFVKSHFSRQDILDLWIYHTQGYFPQ"
            }
        },
        {
            "id": "workspace_object_ref",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "workspaceObjectRef",
            "text_options": {
                "valid_ws_types": ["ProteinExistenceResult", "ProteinEmbeddingResult"],
                "example": "Select a workspace object containing protein data"
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kbase_protein_query_module",
            "method": "generate_protein_embedding",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "input_type",
                    "target_property": "input_type"
                },
                {
                    "input_parameter": "sequence",
                    "target_property": "sequence"
                },
                {
                    "input_parameter": "workspace_object_ref",
                    "target_property": "workspace_object_ref"
                }
            ],
            "output_mapping": [
                { "service_method_output_path": [0, "embedding_result_ref"], "target_property": "embedding_result_ref" },
                { "service_method_output_path": [0, "summary"], "target_property": "summary" },
                { "service_method_output_path": [0, "input_parameters"], "target_property": "input_parameters" },
                { "service_method_output_path": [0, "start_time"], "target_property": "start_time" },
                { "service_method_output_path": [0, "embedding_norm"], "target_property": "embedding_norm" },
                { "service_method_output_path": [0, "sequence_length"], "target_property": "sequence_length" },
                { "service_method_output_path": [0, "embedding_dim"], "target_property": "embedding_dim" }
            ]
        }
    },
    "job_id_output_field": "docker"
}