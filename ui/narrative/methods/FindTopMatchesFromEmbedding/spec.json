{
    "ver": "0.0.1",
    "authors": [
        "yourname"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active", "protein", "search"],
    "widgets": {
        "input": null,
        "output": null
    },
    "parameters": [
        {
            "id": "input_type",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["embedding"],
            "field_type": "dropdown",
            "text_options": {
                "valid_ws_types": [],
                "example": "Choose input type",
                "options": [
                    {"value": "embedding", "display": "Protein Embedding"},
                    {"value": "existence_result", "display": "Protein Existence Result"},
                    {"value": "family_assignment", "display": "Family Assignment Result"}
                ]
            }
        },
        {
            "id": "embedding_ref",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "workspaceObjectRef",
            "text_options": {
                "valid_ws_types": ["ProteinEmbeddingResult"],
                "example": "Select a previously generated embedding."
            }
        },
        {
            "id": "existence_result_ref",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "workspaceObjectRef",
            "text_options": {
                "valid_ws_types": ["ProteinExistenceResult"],
                "example": "Select a protein existence result."
            }
        },
        {
            "id": "family_assignment_ref",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "workspaceObjectRef",
            "text_options": {
                "valid_ws_types": ["ProteinFamilyAssignmentResult"],
                "example": "Select a family assignment result."
            }
        },
        {
            "id": "family_id",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [],
                "example": "FAM12345 or leave empty to use family from input object"
            }
        },
        {
            "id": "top_n",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["10"],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int": 1,
                "max_int": 1000
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kbase_protein_query_module",
            "method": "find_top_matches_from_embedding",
            "input_mapping": [
                { "narrative_system_variable": "workspace", "target_property": "workspace_name" },
                { "input_parameter": "input_type", "target_property": "input_type" },
                { "input_parameter": "embedding_ref", "target_property": "embedding_ref" },
                { "input_parameter": "existence_result_ref", "target_property": "existence_result_ref" },
                { "input_parameter": "family_assignment_ref", "target_property": "family_assignment_ref" },
                { "input_parameter": "family_id", "target_property": "family_id" },
                { "input_parameter": "top_n", "target_property": "top_n" }
            ],
            "output_mapping": [
                { "service_method_output_path": [0, "matches"], "target_property": "matches" },
                { "service_method_output_path": [0, "summary"], "target_property": "summary" },
                { "service_method_output_path": [0, "input_parameters"], "target_property": "input_parameters" },
                { "service_method_output_path": [0, "start_time"], "target_property": "start_time" },
                { "service_method_output_path": [0, "family_id"], "target_property": "family_id" },
                { "service_method_output_path": [0, "top_n"], "target_property": "top_n" },
                { "service_method_output_path": [0, "similarity_stats"], "target_property": "similarity_stats" },
                { "service_method_output_path": [0, "similarity_search_result_ref"], "target_property": "similarity_search_result_ref" }
            ]
        }
    },
    "job_id_output_field": "docker"
}
